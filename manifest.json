{
  "name": "JCGuerrero",
  "description": "My Chrome Sandbox Extensions",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting"],
  "background": {
    "service_worker": "background/service_worker.js"
  },
  "options_page": "options/default.html",
  "action": {
    "default_popup": "popup/default.html"
  }
}
