{
  "name": "chrome-extension-tutorials-app",
  "description": "Getting started with Chrome Extensions",
  "version": "0.1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting"],
  "background": {
    "service_worker": "background/service_worker.min.js"
  },
  "options_page": "options/options.html",
  "action": {
    "default_popup": "popups/default/default_popup.html"
  }
}
